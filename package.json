{
  "dependencies": {
    "@remix-run/express": "1.1.1",
    "@remix-run/react": "1.1.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.17.2",
    "morgan": "1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.1.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.0",
    "@tailwindcss/typography": "0.5.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "autoprefixer": "10.4.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-remix": "0.0.2",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.0.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.5",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.8",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "",
  "lint-staged": {
    "*.+(js|ts|tsx|css|md|yml)": [
      "prettier --write"
    ],
    "*.+(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "name": "remix-starter",
  "private": true,
  "scripts": {
    "build": "remix build",
    "dev": "NODE_ENV=development run-p dev:*",
    "dev:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/app.css --watch",
    "dev:remix": "remix watch",
    "dev:server": "cross-env NODE_ENV=development node server/index.js",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "postinstall": "remix setup node",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "typecheck": "tsc"
  },
  "sideEffects": false
}
